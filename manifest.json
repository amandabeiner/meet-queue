{
  "manifest_version": 2,
  "name": "Meet Queue",
  "description": "Queueing system for Google Meet",
  "version": "0.0.1",
  "background": {
    "scripts": ["socket.io.min.js", "auth.js", "background.js"]
  },
  "browser_action": {},
  "content_scripts": [
    {
      "matches": ["https://meet.google.com/*"],
      "js": ["styles.js", "content.js"],
      "css": ["output.css"]
    }
  ],
  "web_accessible_resources": ["icon-gray.png", "icon-green.png"],
   "content_security_policy": "script-src 'self'  https://apis.google.com https://www.googleapis.com;  object-src 'self'",
  "permissions": ["tabs", "activeTab", "debugger", "identity", "*://www.googleapis.com/*", "*://accounts.google.com/*"],
  "oauth2": {
    "client_id": "390532946027-88l9gde3l22dee5o7t6t0g5m8ajqmb8r.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]

  },
  "key": ""

}
 
